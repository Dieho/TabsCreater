<Window x:Class="TabsCreator.Windows.TunerWindow"
        xmlns:myNamespace="clr-namespace:TabsCreator.Controls"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TabsCreator"
        mc:Ignorable="d"
        MouseLeftButtonDown="Window_MouseLeftButtonDown"
        IsVisibleChanged="TunerWindow_OnIsVisibleChanged"
        Title="Tuner" Height="300" Width="450" Background="#FFE6E6E6" WindowStyle="None" ResizeMode="NoResize" BorderThickness="1" BorderBrush="Black" >
    <Grid Margin="0,0,0,0" Background="#FFE6E6E6">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="269*"/>
            <ColumnDefinition Width="173*"/>
        </Grid.ColumnDefinitions>
        <myNamespace:FrequencyScale x:Name="FrequencyScale" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
