<Window x:Class="TabsCreator.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TabsCreator"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Background="#FFE6E6E6">
    <Grid>
        <Button x:Name="SelectDeviceButton" Content="Select device" HorizontalAlignment="Left" Margin="432,37,0,0" VerticalAlignment="Top" Width="75" Click="SelectDeviceButton_Click" Background="#FFE6E6E6"/>
        <Button x:Name="TunerButton" Content="Tuner" HorizontalAlignment="Left" Margin="432,72,0,0" VerticalAlignment="Top" Width="75" Click="TunerButton_Click"/>
        <Button x:Name="StopButton" Content="Stop" HorizontalAlignment="Left" Margin="432,106,0,0" VerticalAlignment="Top" Width="75" Click="StopButton_Click"/>
    </Grid>
</Window>
